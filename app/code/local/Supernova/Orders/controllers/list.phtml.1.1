function increaseVessalePositionInch(sink){
	
		var vessle_faucet_position_inch = parseFloat(jQuery('#vessle_faucet_position_inch_'+sink).val());

		if(vessle_faucet_position_inch >= 5){
			jQuery('.error_message_box').html('');
		}

		vessle_faucet_position_inch += 0.25;
		jQuery('#vessle_faucet_position_inch_'+sink).val(vessle_faucet_position_inch);
		jQuery('.sink_'+sink+'_faucet_distance').html('Sink '+sink+' : '+roundVal(vessle_faucet_position_inch)+'"');

}

function decreaseVessalePositionInch(sink){
	
		var vessle_faucet_position_inch = parseFloat(jQuery('#vessle_faucet_position_inch_'+sink).val());
		
		if(vessle_faucet_position_inch <= 5){
			jQuery('.error_message_box').html('Minimum Vessle sink faucet position 5" from center');
			return false;	
		}else{
			jQuery('.error_message_box').html('');
			
		}
		
		vessle_faucet_position_inch -= 0.25;
		jQuery('#vessle_faucet_position_inch_'+sink).val(vessle_faucet_position_inch);
		jQuery('.sink_'+sink+'_faucet_distance').html('Sink '+sink+' : '+roundVal(vessle_faucet_position_inch)+'"');

}

function backSplash(type,flag){
	
		if(!jQuery("#backsplash-"+type+"-input").is(':checked')) {
			jQuery("#backsplash-"+type+"-input").attr('checked','checked');
			jQuery(".backsplash-"+type).css('background','#f9b13f');
			jQuery(".backsplash-"+type+"-egde").show();
			jQuery('#selected-splash .'+flag).html(type);
		}else{
			jQuery("#backsplash-"+type+"-input").removeAttr('checked');
			jQuery(".backsplash-"+type).css('background','#DCDCDC');			
			jQuery(".backsplash-"+type+"-egde").hide();
			jQuery('#selected-splash .'+flag).html('');
		}
}

function polished(type,flag){
	
		if(!jQuery("#polished-"+type+"-input").is(':checked')) {
			jQuery("#polished-"+type+"-input").attr('checked','checked');
			jQuery(".polished-"+type).css('background','#92c3d7');
