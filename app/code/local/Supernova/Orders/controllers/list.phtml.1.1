function changeWidth(){

	val = parseFloat(jQuery('#width').val()) + parseFloat(jQuery('#width-precision').val());
	
	
	if(val<15){
		jQuery('.error_message_box').html('Min Width should be 15');
		jQuery('input#width').css('border','1px solid #ff0000');
		//jQuery('.width_max_error').hide();
		//jQuery('.width_min_error').show();
	}
	else if(val>98){
		jQuery('.error_message_box').html('Max Width should be 98 ');
		jQuery('input#width').css('border','1px solid #ff0000');
		//jQuery('.width_min_error').hide();
		//jQuery('.width_max_error').show();
	}
	else{
		jQuery('.error_message_box').html('');
		jQuery('input#width').css('border','');
		//jQuery('.width_error').hide();
	}
	
	if(val<21){
		jQuery('.sink-container .box').hide();
		jQuery('.sink-container .vassle').show();
		jQuery('#sink_1').click();
		jQuery('#sink_2').attr('disabled','disabled');
	}else if(val<28){
		jQuery('.sink-container .box').hide();
		jQuery('.sink-container .oval').show();
		jQuery('.sink-container .ract').show();
		jQuery('.sink-container .vassle').show();
		jQuery('#sink_1').click();
		jQuery('#sink_2').attr('disabled','disabled');
	}else if(val>=28 && val<42){
		jQuery('.sink-container .box').hide();
		jQuery('.sink-container .oval-big').show();
		jQuery('.sink-container .ract-big').show();
		jQuery('.sink-container .oval').show();
		jQuery('.sink-container .ract').show();
		jQuery('.sink-container .vassle').show();
		jQuery('#sink_1').click();
		jQuery('#sink_2').attr('disabled','disabled');
	}
	else if(val>=42 && val<56){
		jQuery('.sink-container .box').hide();
		jQuery('.sink-container .oval').show();
		jQuery('.sink-container .ract').show();
		jQuery('.sink-container .vassle').show();
		jQuery('#sink_2').removeAttr('disabled','disabled');
	}
	else if(val>=56){
		jQuery('.sink-container .box').hide();
		jQuery('.sink-container .oval-big').show();
		jQuery('.sink-container .ract-big').show();
		jQuery('.sink-container .oval').show();
		jQuery('.sink-container .ract').show();
		jQuery('.sink-container .vassle').show();
		jQuery('#sink_2').removeAttr('disabled','disabled');
	}
	
	else{
		jQuery('#sink_2').removeAttr('disabled','disabled');
		jQuery('.sink-container .box').show();	
	}
	
	if(val<=98 && val>=15){
	val=val*8;
	
	jQuery('.shink-main-width').css('width',val+'px');
	jQuery('.shink-main-width span').html(roundVal((val/8))+'"');
	
	jQuery('div#selected-width').html('Width: '+roundVal((val/8))+'"');
	jQuery('input#selected-width').val(val/8);

	val=val+1;
	jQuery('#shink-main-container').css('width',val+'px');
	//jQuery('.shink-content-container').css('width',val+'px');

	}
	jQuery('#vessle-faucet-position-none').click();
	shinkVertCenter();
	shinkHorizontalCenter();	
	
}

