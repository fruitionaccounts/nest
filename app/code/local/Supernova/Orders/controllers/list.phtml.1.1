function shinkVertCenter(){

	var shrink_main_height = jQuery('#shink-main-container').height();
	var sink_img_height = jQuery('#shink-main-container #dg-1 img').height();
	var remaining_height = shrink_main_height - sink_img_height;
	remaining_height = remaining_height/2;
	jQuery('#shink-main-container #dg-1 img').css('margin-top',remaining_height);
	jQuery('#shink-main-container #dg-2 img').css('margin-top',remaining_height);
		
	var sink_one_margin_top = parseFloat(remaining_height) + parseFloat(jQuery('#shink-main-container #dg-1 img').height()/2)
	sink_one_margin_top = sink_one_margin_top/8;
	sink_one_margin_top = round_up(sink_one_margin_top,2);
	
	jQuery('#sink_one_margin_top').val(roundVal(sink_one_margin_top));
	jQuery('#sink_two_margin_top').val(roundVal(sink_one_margin_top));
	

	jQuery('.shink-1-top-distance span').html(roundVal(sink_one_margin_top)+'"');
	jQuery('.shink-2-top-distance span').html(roundVal(sink_one_margin_top)+'"');
	
	jQuery('#selected_no_of_sink_vessle #sink_1_distance').html('Drain 1: '+roundVal(sink_one_margin_top)+'"');		
	jQuery('#selected_no_of_sink_vessle #sink_2_distance').html('Drain 2: '+roundVal(sink_one_margin_top)+'"');		
	
	jQuery('#vessle-sink-1-top-distance').val(sink_one_margin_top);
	jQuery('#vessle-sink-2-top-distance').val(sink_one_margin_top);
	
	jQuery('#sink_1_position_top').val(sink_one_margin_top);
	jQuery('#sink_2_position_top').val(sink_one_margin_top);
	
	
	
	
	if(jQuery('#shink-main-container #dg-1 img').height() > 0){
	jQuery('.shink-top-space').show();
	}
	
	jQuery('.shink-1-top-space').height(remaining_height);
	jQuery('.shink-2-top-space').height(remaining_height);
	
	var inch = parseFloat(remaining_height)/8;
	inch = round_up(inch,1);
	
	
	jQuery('.shink-1-top-space span').html(roundVal(inch)+'"');
	jQuery('.shink-2-top-space span').html(roundVal(inch)+'"');
	
	
	
}

function shinkHorizontalCenter(){

	var shrink_main_width = jQuery('#shink-main-container').width();

	var sink_img_width = jQuery('#shink-main-container #dg-1 img').width();

	var margin_left = '';

	if(jQuery("#sink_2").is(':checked')) {
		var remaining_width = shrink_main_width - (sink_img_width*2);
		remaining_width = remaining_width/4;
		margin_left = remaining_width;
		remaining_width = remaining_width + (sink_img_width/2);
		if(jQuery('#shink-main-container img').height() > 0){
			jQuery('.shink-2-distance').show();
		}
	}else{
		var remaining_width = shrink_main_width - (sink_img_width);	
		remaining_width = remaining_width/2;
		margin_left = remaining_width;
		remaining_width = remaining_width + (sink_img_width/2);
		if(jQuery('#shink-main-container img').height() > 0){
			jQuery('.shink-2-distance').hide();
		}
	}
	jQuery('#shink-main-container img').css('margin-left',margin_left);
	
	if(jQuery('#shink-main-container #dg-1 img').height() > 0){
		jQuery('.shink-left-space').show();
	}
	
	jQuery('.shink-1-left-space').width(remaining_width);
	jQuery('.shink-2-left-space').width(remaining_width);

	jQuery('.shink-1-top-space').css('left',remaining_width);
	jQuery('.shink-2-top-space').css('left',remaining_width);
	
	var shink_1_container_width = jQuery('#shink-1-container').width();
	shink_1_container_width = shink_1_container_width + remaining_width-1;

	jQuery('#shink-2-container .shink-2-top-space').css('left',shink_1_container_width);

	
